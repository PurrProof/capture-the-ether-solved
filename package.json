{
  "name": "capture-the-ether-solved",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "node": "hardhat node --network hardhat",
    "clean": "rimraf ./artifacts ./cache ./coverage ./typechain-types ./coverage.json && pnpm typechain",
    "clean:all": "rimraf ./artifacts ./cache ./coverage ./typechain-types ./coverage.json ./node_modules",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"test/**/*.ts\" && pnpm typechain",
    "size": "pnpm hardhat size-contracts",
    "lint": "pnpm lint:tsc && pnpm lint:eslint && pnpm prettier:check && pnpm lint:sol",
    "lint:sol": "solhint --max-warnings 0 -f table \"contracts/**/*.sol\"",
    "lint:slither": "pnpm exec ./slither.sh",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "postcompile": "pnpm typechain",
    "prettier:check": "prettier --check \"**/*.{js,json,md,sol,ts,yml}\" --log-level log",
    "prettier:write": "prettier --write \"**/*.{js,json,md,sol,ts,yml}\"",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "test": "hardhat test"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.12",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.11",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.7.4",
    "chai": "^4.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "ethers": "^6.13.3",
    "hardhat": "^2.22.12",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.10",
    "prettier": "^3.3.3",
    "prettier-plugin-solidity": "^1.4.1",
    "rimraf": "^6.0.1",
    "solhint": "^5.0.3",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "^0.8.13",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^7.18.0"
  }
}
